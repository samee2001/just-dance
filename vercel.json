{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "vercel-php@0.6.0"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|img|lib|scss|build|favicon.ico|robots.txt)(.*)",
            "dest": "public/$1$2"
        },
        {
            "src": "/(.*)",
            "dest": "public/index.php"
        }
    ],
    "outputDirectory": "public"
}
