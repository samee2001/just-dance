{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "public/build/$1"
        },
        {
            "src": "/frontend/(.*)",
            "dest": "public/frontend/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "public/index.php"
        }
    ],
    "outputDirectory": "public"
}
